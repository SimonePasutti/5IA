<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="FRMdialog" prefHeight="474.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatfx.FXMLDialogController">
   <children>
      <Label fx:id="lblActualUser" layoutX="14.0" layoutY="14.0" text="User">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ListView fx:id="listMessages" layoutX="14.0" layoutY="89.0" prefHeight="232.0" prefWidth="670.0" />
      <TextField fx:id="txtDestination" layoutX="14.0" layoutY="52.0" prefHeight="26.0" prefWidth="670.0" promptText="destination" />
      <Label fx:id="lblSendingDest" layoutX="15.0" layoutY="36.0" text="sending to...">
         <font>
            <Font name="System Italic" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtMessage" layoutX="12.0" layoutY="352.0" prefHeight="26.0" prefWidth="670.0" promptText="wirte your message" />
      <Button fx:id="btnSend" layoutX="12.0" layoutY="391.0" mnemonicParsing="false" onAction="#handleSendMessage" prefHeight="26.0" prefWidth="670.0" text="Send" />
      <Label fx:id="lblErrors" layoutX="15.0" layoutY="321.0" textFill="#e10101" />
      <Button fx:id="btnLogout" layoutX="12.0" layoutY="427.0" mnemonicParsing="false" onAction="#handleLogout" prefHeight="26.0" prefWidth="200.0" text="Logout" />
      <Button fx:id="btnWhoIsIn" layoutX="247.0" layoutY="427.0" mnemonicParsing="false" onAction="#handleWhoIsIn" prefHeight="26.0" prefWidth="200.0" text="Who is in" />
      <Button fx:id="btnBroadCast" layoutX="483.0" layoutY="428.0" mnemonicParsing="false" onAction="#handleBroadcast" prefHeight="26.0" prefWidth="200.0" text="Broadcast" />
   </children>
</AnchorPane>
